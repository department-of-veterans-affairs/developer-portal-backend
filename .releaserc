{
    "branch": "master",
    "plugins": [
        "@semantic-release/commit-analyzer",
        "@semantic-release/release-notes-generator",
        [
            "@semantic-release/changelog",
            {
                "changelogFile": "CHANGELOG.md",
                "changelogTitle": "# Semantic Versioning Changelog"
            }
        ],
        [
            "@semantic-release/github",
            {
                "successComment": false,
                "failComment": false,
                "failTitle": false,
                "labels": false,
                "releasedLabels": false
            }
        ],
        [
            "@semantic-release/exec",
            {
                "prepareCmd": "python3 cicd/tag_containers.py -v ${nextRelease.version} -i ${nextRelease.gitHead} -r dvp/developer-portal-backend -n dev-portal-backend-ci",
                "publishCmd": "cicd/deploy_to_ecs.sh ${nextRelease.version} dev-portal-be dvp-dev"
            }
        ]
    ]
}
