{
  "name": "developer-portal-backend",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node dist/server",
    "watch": "nodemon --watch '*.ts' --exec 'ts-node' server.ts",
    "watch:debug": "nodemon --watch '*.ts' --exec 'node --require ts-node/register --inspect-brk=0.0.0.0:9229 server.ts'",
    "lint": "NODE_OPTIONS=\"--max_old_space_size=1536\" eslint --ext .js,.ts .",
    "test": "jest",
    "test:debug": "node --inspect-brk=0.0.0.0:9229 node_modules/.bin/jest --runInBand",
    "test-ci": "jest --coverage",
    "test:watch": "jest --watchAll",
    "build": "tsc"
  },
  "dependencies": {
    "@hapi/joi": "^17.1.1",
    "@okta/okta-sdk-nodejs": "^4.1.0",
    "@sentry/node": "^6.2.2",
    "@types/objects-to-csv": "^1.3.0",
    "aws-sdk": "^2.867.0",
    "axios": "^0.21.1",
    "cors": "^2.8.5",
    "express": "~4.17.1",
    "handlebars": "^4.7.3",
    "moment": "^2.27.0",
    "morgan": "~1.10.0",
    "objects-to-csv": "^1.3.6",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@types/command-line-args": "^5.0.0",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.11",
    "@types/handlebars": "^4.1.0",
    "@types/hapi__joi": "^17.1.6",
    "@types/jest": "^26.0.21",
    "@types/morgan": "^1.9.2",
    "@types/node": "^14.14.35",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^4.18.0",
    "@typescript-eslint/parser": "^4.18.0",
    "command-line-args": "^5.1.1",
    "dotenv": "^8.2.0",
    "eslint": "^7.24.0",
    "jest": "^26.6.3",
    "jest-junit-reporter": "^1.1.0",
    "nock": "^13.0.11",
    "nodemon": "^2.0.7",
    "sprintf-js": "^1.1.2",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.4",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.3"
  }
}
